{
	"name": "carlogoquiz-api-dev",
	"workers_dev": true,
	"main": "src/index.ts",
	"compatibility_date": "2025-01-29",
	"upload_source_maps": true,
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["Session"]
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "SESSION_DO",
				"class_name": "Session"
			}
		]
	},
	"observability": {
		"enabled": true
	},
	"env": {
		"dev": {
			"name": "carlogoquiz-api-dev",
			"workers_dev": true,
			"assets": {
				"directory": "./assets"
			},
			"vars": {
				"MEDIA_BASE_URL": "http://localhost:8787",
				"PRODUCTION": "false",
				"BRANDS_CACHE_DURATION": "10"
			},
			"durable_objects": {
				"bindings": [
					{
						"name": "SESSION_DO",
						"class_name": "Session"
					}
				]
			},
			"kv_namespaces": [
				{
					"binding": "BRANDS_KV",
					"id": "my-dev-kv-id"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "CarQuizHub-dev",
					"database_id": "my-dev-d1-id",
					"migrations_table": "d1_migrations",
					"migrations_dir": "migrations"
				}
			]
		},
		"production": {
			"name": "carlogoquiz-api",
			"workers_dev": false,
			"vars": {
				"MEDIA_BASE_URL": "https://my-production-r2-bucket-url",
				"PRODUCTION": true,
				"BRANDS_CACHE_DURATION": "604800"
			},
			"durable_objects": {
				"bindings": [
					{
						"name": "SESSION_DO",
						"class_name": "Session"
					}
				]
			},
			"kv_namespaces": [
				{
					"binding": "BRANDS_KV",
					"id": "my-prod-kv-id"
				}
			],
			"d1_databases": [
				{
					"binding": "DB",
					"database_name": "CarQuizHub",
					"database_id": "my-production-database-id",
					"migrations_table": "d1_migrations",
					"migrations_dir": "migrations"
				}
			]
		}
	}
}
